			<script>
				$(function(){
					$(document).on('click', '#get-user-needs', function(e){
						StatisticsData(115, 14);
						$('#modal-alert-solution-box').modal('show');
					})
			
					$('input[type=radio][name="solutionType"]').off().change(function() {
						if (this.value == 9) {
							$('#other-input').removeClass('solution-none').show();
						}
					});
			
					$(document).on('click', '#publish-solution-cancel-btn', function() {
						StatisticsData(115,12);
					})
			
					$(document).on('click', '#publish-solution-success-btn', function() {
						StatisticsData(115,17);
					})
			
					$(document).on('hide.bs.modal', '#modal-alert-solution-box', function () {
						StatisticsData(115,13);
					});
			
					$(document).on('click', '#publish-solution-submit-btn', function () {
						StatisticsData(115,15);
						var solution_value = $('input[type=radio][name="solutionType"]:checked').val();
						var use_scene_input = $('#use-scene').val();
			
						if (!solution_value) {
							showError('请添加你的行业');
							return false;
						}
			
						if (solution_value == 9) {
							var other_input = $('#other-input').val();
						   
			
							if (!other_input) {
								showError('请输入应用场景');
								return false;
							}
			
							save_solution_template(9, other_input, use_scene_input);
			
							return;
						}
			
						save_solution_template(solution_value, '', use_scene_input);
					})
			
					function getCookie(name) {
						var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
						if (arr != null) {
							return (arr[2]);
						}
					}
			
					function save_solution_template(solution_type, other_info, user_scene) {
						$('#publish-solution-submit-btn').button('loading');
			
						var _cvid = getCookie('cvid');
			
						$.ajax({
							type: 'GET',
							url: '//' + CLI_DOMAIN + '/Api/LogIndustry/log',
							async: true,
							data: {
								'industry_id': solution_type,
								'industry_need': other_info,
								'industry_solve': user_scene,
								'cvid': _cvid,
							},
							dataType: 'json',
							success: function(ret) {
								if (ret.status) {
									$('#publish-solution-submit-btn').button('reset');
									$('#modal-alert-solution-box').modal('hide');
									$('#modal-alert-solution-success-box').modal('show');
									StatisticsData(115, 16);
								}
							},
							error: function(e) {
							}
						});
					}
			
				})
			
				function showError(msg) {
					$('#error-message-solution')
					.text(msg)
					.css('display', 'inline-block')
					.delay(2000)
					.fadeOut();
				}
			
				function StatisticsData(fir,sec){
					$.ajax({
						async: true,
						type: "get",
						url: '/Api/ClickLog/click',
						data: {
							'fir': fir,
							'sec': sec
						},
						dataType: 'jsonp',
						jsonp: 'callback',
						success: function(ret) {
						}
					});
				}
			</script>
