    $('[use-quickly-machine]').click(function() {
        var api_host = '',
            STATIC_SERVICE = '',
            now_host = window.location.host;
            if (now_host.indexOf('cli.me') != -1) {
                api_host = 'cli.me';
                STATIC_SERVICE = 'static.cli.me/cli'; //测试--本地
            } else if (now_host.indexOf('cliim.net') != -1) {
                api_host = 'cliim.net';
                STATIC_SERVICE = 'static-develop.clewm.net/cli'; //测试--net
            } else if (now_host.indexOf('cliim.com') !== -1) {
                api_host = 'cliim.com';
                STATIC_SERVICE = 'static-test.clewm.net/cli'; //测试--com
            } else {
                api_host = 'cli.im';
                STATIC_SERVICE = 'static.clewm.net/cli';
            }
        if (account == false) { //未登录
            seajs.use('//'+STATIC_SERVICE+'/js/login_plugin.js?v=20190610', function(cli) {
                // change_type 为 13
                cli.login_plugin('join', 12); // 带提示的登录
            });
        } else { //已登陆
            //1，判断有没有到期，显示相应的弹窗 2，如果是完善信息的话，请求数据的借口 3，完成
            
            $.ajax({
                type: "get",
                url: '//user.' + api_host + '/api/index/get_probation_info',
                dataType: "jsonp",
                async: false,
                success: function(ret) {
                    var is_test = ret.is_test;
                    if (is_test == 0) {
                        //判断是否完善过信息  is_full=1表示已完善过信息
                        $.ajax({
                            type: "get",
                            url: '//user.' + api_host + '/api/index/get_biz_info',
                            dataType: "jsonp",
                            success: function(ret) {
                                var is_full = ret.is_full;
                                if (is_full == 1 && ret.industry.id == $('input.industry').val()) {
                                    openSolution();
                                } else {
                                  var industryId = $('input.industry').val();
                                  var caseId = $('input#case').val();
                                  window.location.href = '//'+ api_host +'/solution/index/free_trial?industry='+industryId + '&case=' + caseId;
                                  // 弹出完善信息的框
                                  // buildInfo.add_build_box_common('logined');
                                }
                            }
                        });
                    } else if (is_test == 1 || is_test == 2) {
                        window.location.href = '//biz.'+ api_host +'/product';
                    } else if (is_test == 3) {
                        ViewFreeDialogInfo('ExpiredFreeFront');
                    }
                }
            });

        }
    });
</script>
